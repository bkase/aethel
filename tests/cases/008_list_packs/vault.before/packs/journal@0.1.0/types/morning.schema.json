{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "properties": {
    "uuid": { "type": "string" },
    "type": { "type": "string" },
    "created": { "type": "string" },
    "updated": { "type": "string" },
    "v": { "type": "string" },
    "tags": { "type": "array" },
    "mood": {
      "type": "string",
      "enum": ["happy", "sad", "neutral", "excited"]
    },
    "energy": {
      "type": "integer",
      "minimum": 1,
      "maximum": 10
    }
  },
  "additionalProperties": false
}